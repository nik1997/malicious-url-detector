{
    "manifest_version": 2,

    "name": "malicious-url-detector",
    "version": "1.0",
    "description": "A extension that detects malicious URL using machine learning.",

    "background": {
        "scripts" : ["tf.min.js", "background.js"]
    },

    "browser_action": {
        "default_popup": "popup.html"
    },


    "permissions": [
        "tabs"
    ],

    "web_accessible_resources": [
        "model/tokenInfo.json"
    ]
}