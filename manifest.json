{
    "manifest_version": 2,

    "name": "malicious-url-detector",
    "version": "1.0",
    "description": "A extension that detects malicious URL using machine learning.",

    "background": {
        "scripts" : ["background.js"]
    },

    "browser_action": {
        "default_popup": "popup.html"
    },

    "content_scripts": [ {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["contentscript.js"]
      } ],

    "permissions": [
        "tabs"
    ]
}